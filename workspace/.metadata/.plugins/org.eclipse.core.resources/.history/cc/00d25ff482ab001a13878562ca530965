package training.day2;

// any class in the world 
// if you dont extend 
// then that class will be the 
// child of Object class 
public class Employee {
	
	Employee() {
		// if you have super 
		// in your constructor 
		//then this should 
		// be first statement 
		System.out.println("hi");
		super();
		
	}
	int empId; 
	String empName; 
	double empSal; 
}
