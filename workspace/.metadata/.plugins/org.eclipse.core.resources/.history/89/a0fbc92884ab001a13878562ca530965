package training.day2;

// any class in the world 
// if you dont extend 
// then that class will be the 
// child of Object class 
public class Employee {
	private int empId; 
	private String empName; 
	private double empSal; 
	
	Employee() {
		// if you have super 
		// in your constructor 
		//then this should 
		// be first statement 
		super();
		
		System.out.println("Object created... ");
		
	}
}
