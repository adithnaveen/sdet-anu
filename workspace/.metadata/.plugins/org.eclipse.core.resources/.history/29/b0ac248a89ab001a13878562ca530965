package training.day2.has;

public class Client {
	public static void main(String[] args) {
		
		Customer cust = new Customer(); 
		
		cust.setCustId(101);
		cust.setIncome(2000);
		
		Name name  = new Name(); 
		name.setFirstName("Anu");
		name.setLastName("Mehta");
		
		cust.setName(name);
		
		
	}
}
