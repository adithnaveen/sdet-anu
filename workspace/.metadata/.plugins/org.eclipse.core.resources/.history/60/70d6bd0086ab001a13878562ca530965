package training.day2;

public class EmployeeImpl {
	public static void main(String[] args) {
		Employee emp = new Employee(); 
		
		emp.setEmpId(101);
		emp.setEmpName("A");
		emp.setEmpSal(4000);
		
		// emp.display();
		System.out.println("Emp Id : " + emp.getEmpId());
		System.out.println("Emp Name : " + emp.getEmpName());
		
	}
}
